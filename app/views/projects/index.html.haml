%table.projects
  %thead
    %tr
      %th Name
      %th
      %th
  %tbody
    - @projects.each do |project|
      %tr
        %td= link_to project, project
        %td= link_to 'Edit', edit_project_path(project)
        %td= link_to 'Destroy', project_path(project), :method => :delete

= link_to "New Project", new_project_path
