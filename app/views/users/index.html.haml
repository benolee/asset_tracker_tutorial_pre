%h2 All Users
.gradient_border_wrap
  .gradient_border_content
    %table.users
      %thead
        %tr
          %th Name
          %th Email
      %tbody
        - @users.each do |user|
          %tr
            %td= link_to user.to_s, admin_user_path(user)
            %td= user.email
    = button_to "New User", new_admin_user_path, :class => "add_new_button", :method => :get
