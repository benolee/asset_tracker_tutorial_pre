%h2= "Historical Time Entered for #{@user.to_s}"
.gradient_border_wrap
  .gradient_border_content
    - (0..5).each do |x|
      - start_date = Date.today.beginning_of_week - x.weeks
      %h3= "Week of #{start_date} to #{start_date + 1.week}"
      = render :partial => 'shared/calendar', :locals => { :start_date => start_date, :user => @user }
