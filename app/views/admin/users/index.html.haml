%h1
  All Users

%table.users
  %thead
    %tr
      %th Name
      %th Email
      %th
      %th
  %tbody
    - @users.each do |user|
      %tr
        %td= link_to (user.first_name+ " " + user.last_name), admin_user_path(user)
        %td= user.email
        %td= link_to 'Edit', edit_admin_user_path(user)
        %td

= link_to "New Client", new_client_path
